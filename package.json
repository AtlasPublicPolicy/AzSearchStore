{
  "name": "azsearchstore",
  "version": "0.0.1",
  "description": "redux based UI state management for applications consuming Azure Search",
  "main": "dist/AzSearchStore.js",
  "browser": "dist/bundle.js",
  "typings": "dist/AzSearchStore.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest",
    "start_server": "http-server"
  },
  "author": "Evan Boyle (https://github.com/EvanBoyle)",
  "license": "MIT",
  "devDependencies": {
    "@types/nock": "^8.2.0",
    "@types/redux-mock-store": "0.0.7",
    "@types/es6-promise": "0.0.32",
    "@types/isomorphic-fetch": "0.0.32",
    "@types/jest": "^16.0.4",
    "@types/object-assign": "^4.0.30",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.20",
    "@types/react-redux": "^4.4.36",
    "@types/redux-thunk": "^2.1.0",
    "@types/urijs": "^1.15.31",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "jest": "^18.1.0",
    "nock": "^9.0.2",
    "redux-mock-store": "^1.2.2",
    "source-map-loader": "^0.1.6",
    "ts-jest": "^18.0.2",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "object-assign": "^4.1.1",
    "es6-promise": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "urijs": "^1.18.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "collectCoverage": true
  }
}